{
  "expo": {
    "name": "padel-match-mobile",
    "slug": "cliente-padel-match-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow PORT MATCH to use your location."
        }
      ]
      ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "jsEngine": "hermes",
      "package": "com.sportmatch.app"
    },
    "ios": {
      "jsEngine": "hermes",
      "bundleIdentifier": "com.sportmatch.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicaci칩n para mostrarte los clubes m치s cercanos a ti.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Necesitamos tu ubicaci칩n para mostrarte los clubes m치s cercanos a ti."
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1ebe1597-6057-4267-ad8f-c5b24c6969db"
      }
    },
    "owner": "tomisteven"
  }
}